{
  "version": 2,
  "name": "uncssOnline",
  "alias": "uncss-online.com",
  "routes": [
    { "src": "/api/uncss/", "dest": "/api/uncss/", "methods": ["POST"] },
    { "src": "/(.*)", "dest": "/dist/$1" }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "dist" }
    },
    { "src": "/api/uncss/index.js", "use": "now-micro" }
  ]
}
